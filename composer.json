{
    "name": "asker26/rocket-router",
    "description": "A fast and flexible PHP router package with attribute-based routing and automatic route generation",
    "type": "library",
    "keywords": ["php", "router", "routing", "http", "attributes", "api", "rest"],
    "homepage": "https://github.com/asker26/rocket-router",
    "license": "MIT",
    "authors": [
        {
            "name": "Asker Ali",
            "email": "askeraliyev24@proton.me",
            "homepage": "https://github.com/asker26",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/asker26/rocket-router/issues",
        "source": "https://github.com/asker26/rocket-router"
    },
    "require": {
        "php": "7.4.33|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "RocketRouter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RocketRouter\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "bin/generate-routes"
    ],
    "config": {
        "sort-packages": true
    }
}

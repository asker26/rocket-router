{
    "name": "asker/rocket-router",
    "description": "A fast and flexible PHP router package",
    "type": "library",
    "keywords": ["php", "router", "routing", "http"],
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "RocketRouter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RocketRouter\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "bin/generate-routes"
    ],
    "config": {
        "sort-packages": true
    }
}
